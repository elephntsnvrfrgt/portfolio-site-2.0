mixin mixin_hero(eyebrow, title, info)
	.herotext
		.eyebrow= eyebrow
		h2.title= title
		.info= info

mixin mixin_experience(job_name, years)
	li.experience
		p= job_name
		.small= years

mixin mixin_js
	script(src='https://cdnjs.cloudflare.com/ajax/libs/gsap/1.20.3/TweenMax.min.js')
	script(src='assets/js/nav.js')
	script(src='assets/js/CustomEase.min.js')
